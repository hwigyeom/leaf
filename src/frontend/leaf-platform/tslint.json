{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "object-literal-sort-keys": [
      false
    ],
    "trailing-comma": [
      false
    ],
    "no-console": [
      false
    ],
    "no-empty": [
      true,
      "allow-empty-functions",
      "allow-empty-catch"
    ],
    "max-classes-per-file": [
      false
    ],
    "no-string-literal": [
      false
    ],
    "no-var-requires": [
      false
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  }
}
